{
  "family": "#TASK_DEFINITION_NAME#",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "#CONTAINER_NAME#",
      "image": "#CONTAINER_IMAGE#",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "#TASK_ROLE_ARN#"
}
